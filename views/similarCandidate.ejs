<!DOCTYPE html>
<html lang="en">
<% include header %>
<body>
<% include company_navbar %>
<link rel='stylesheet' href='/stylesheets/style.css' />
<div class="container">
    <% if (result && foundUser.length > 0) {%>
    <div class="row">
        <div class="col-md-8">
            <h3><span class="label label-default">Similar candidates</span></h3>
        </div>
    </div>
            <% for(var i=0; i< foundUser.length; i++) {%>
            <div class="row" >
                <div class="col-md-8">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title"><label ><%=foundUser[i].firstName%> <%=foundUser[i].lastName%></label></h3>
                        </div>
                        <div class="panel-body">
                            <table class="table">
                                <tbody>
                                <tr>
                                    <td><span><img style="height:140px; width:140px;" class="img-thumbnail" src=<%=foundUser[i].imageUrl%>></span></td>
                                    <td><span id="description"><%=foundUser[i].headline%></span></td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <br>
        <%}%> <!--end for loop -->
        <%}else{%>
        <h3><span class="label label-default">No similar candidate found</span></h3>
        <%}%> <!--end else-->
</div>
<!-- Bootstrap core JavaScript
================================================== -->
<!-- Placed at the end of the document so the pages load faster -->
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<!-- Latest compiled and minified JavaScript -->
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
</body>
</html>
    <!DOCTYPE html>
    <html lang="en">
    <% include header %>
    <body>
    <% include profile_navbar %>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <div class="container">
      

        <div class="container">
            <br>
             <p class="bg-danger"><%=errMsg%></p>
			 <form class="form-horizontal" method="post" action="/jobApplications">

			 <div class="row" id="jobsApplied">
			
				  
			<div class="row">
             <div class="col-md-8">
			 <h3><span class="label label-default">Jobs recommend for you</span></h3>
			 </div>
			 <div class="col-md-2">
			<span> <button id="button" onClick="callMethod()" type="submit" class="btn btn-custom-darken">Apply for Selected Job</button></span>
			 </div>
			 </div>
			 
			 <% for(var i=0; i< jobList.length; i++) {%>
			 <div class="row" >
			 <div class="col-md-8">
			<div class="panel panel-default">
			 <div class="panel-heading">
<h3 class="panel-title"><label ><%=jobList[i].position%></label></h3>
</div>
<div class="panel-body">
<table class="table">
<tbody>
<tr>
<td><span><label >Company </label></span> </td>
<td><span id="description"><%=jobList[i].company%></span></td>
</tr>
<tr>
<td><span><label >Description </label></span> </td>
<td><span id="description"><%=jobList[i].description%></span></td>
</tr>
<tr>
<td><span><label >Location </label></span> </td>
<td><span id="location"><%=jobList[i].location%></span></td>
</tr>
<tr>
<td><span><label>Valid till </label></span> </td>
<td><span id="expires"><%=moment(jobList[i].expires).format(shortDateFormat)%></span></td>
</tr>
<tr>
<td><span><label >Id </label></span> </td>
<td><span id="id"><%=jobList[i].jobId%></span></td>
</tr>
</tbody>
</table>
</div>
</div>
</div>
<br>
 
  <div class="checkbox" class="col-md-2">
    <label>
      <input type="radio" name="jobId" id="jobIdSelected" aria-label="..." value="<%=jobList[i].jobId%> <%=jobList[i].companyId%>" >
	        Apply
     </label>
  </div>
  </div>

  <%}%>

</form>
</div>
 </div> <!--container!-->
